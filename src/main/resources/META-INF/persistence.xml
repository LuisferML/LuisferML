<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence           http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
  <persistence-unit name="asowebPersUnit" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.curso.asoweb.logica.CodigoDeSeguridad</class>
    <class>com.curso.asoweb.logica.Dominios</class>
    <class>com.curso.asoweb.logica.EstadosSocios</class>
    <class>com.curso.asoweb.logica.Exposiciones</class>
    <class>com.curso.asoweb.logica.MensajesEnviadosSocios</class>
    <class>com.curso.asoweb.logica.Menu</class>
    <class>com.curso.asoweb.logica.Menus</class>
    <class>com.curso.asoweb.logica.MontosCuota</class>
    <class>com.curso.asoweb.logica.MovimientosSocios</class>
    <class>com.curso.asoweb.logica.Opciones</class>
    <class>com.curso.asoweb.logica.PagosCuotasSocios</class>
    <class>com.curso.asoweb.logica.PagosOtrosConceptos</class>
    <class>com.curso.asoweb.logica.ParticExpoSocios</class>
    <!--<class>com.curso.asoweb.logica.Roles</class>-->
    <class>com.curso.asoweb.logica.Socios</class>
    <class>com.curso.asoweb.logica.TematicaParticExpoSocios</class>
    <class>com.curso.asoweb.logica.TiposMovimiento</class>
    <class>com.curso.asoweb.logica.Usuarios</class>
    <class>com.curso.asoweb.logica.RolesUsuarios</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/asoweb"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="Postgresql121983"/>
      <property name="jakarta.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
